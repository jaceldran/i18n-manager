{
    "name": "zentric/118n-manager",
    "description": "i18n Manager",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Juan Ángel Celdrán",
            "email": "jaceldran@gmail.com"
        }
    ],
    "scripts": {
        "phinx-create": "./vendor/bin/phinx --configuration=\"./boot/phinx.php\" create",
        "phinx-migrate": "./vendor/bin/phinx --configuration=\"./boot/phinx.php\" migrate",
        "phinx-rollback": "./vendor/bin/phinx --configuration=\"./boot/phinx.php\" rollback",
        "phinx-seed-create": "./vendor/bin/phinx --configuration=\"./boot/phinx.php\" seed:create",
        "phinx-seed-run": "./vendor/bin/phinx --configuration=\"./boot/phinx.php\" seed:run"
    },
    "require": {
        "mikecao/flight": "^2.0",
        "eftec/bladeone": "^4.5"
    }
}
